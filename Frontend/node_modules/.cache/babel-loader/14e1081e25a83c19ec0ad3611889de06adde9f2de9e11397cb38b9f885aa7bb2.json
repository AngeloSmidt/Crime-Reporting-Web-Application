lIntoView({\n          behavior: 'smooth',\n          block: 'start'\n        });\n        mobileOpen.value = false;\n      }\n    }\n    onMounted(() => document.addEventListener('keydown', onKey));\n    onBeforeUnmount(() => document.removeEventListener('keydown', onKey));\n    const __returned__ = {\n      get router() {\n        return router;\n      },\n      set router(v) {\n        router = v;\n      },\n      mobileOpen,\n      panicOpen,\n      year,\n      emergencyTel,\n      go,\n      openPanic,\n      closePanic,\n      sendPanic,\n      onKey,\n      scrollToId,\n      ref,\n      onMounted,\n      onBeforeUnmount,\n      computed\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onBeforeUnmount","computed","router","mod","__temp","__restore","_withAsyncContext","catch","useRouter","_","mobileOpen","panicOpen","year","Date","getFullYear","emergencyTel","go","path","push","window","location","href","openPanic","value","navigator","vibrate","closePanic","sendPanic","console","log","onKey","e","key","scrollToId","id","el","document","getElementById","scrollIntoView","behavior","block","addEventListener","removeEventListener"],"sources":["C:/Users/sifis/OneDrive/Documents/Project 3/Crime-Reporting-Web-Application/Frontend/src/pages/Landing.vue"],"sourcesContent":["<template>\r\n  <div class=\"min-h-screen bg-white flex flex-col text-gray-800\">\r\n    <!-- Decorative background -->\r\n    <div aria-hidden=\"true\" class=\"pointer-events-none fixed inset-0 overflow-hidden\">\r\n      <div class=\"brand-gradient blur-3xl opacity-20 absolute -top-24 -right-24 w-[480px] h-[480px] rounded-full\"></div>\r\n      <div class=\"brand-gradient blur-3xl opacity-10 absolute -bottom-24 -left-24 w-[520px] h-[520px] rounded-full animate-float\"></div>\r\n    </div>\r\n\r\n    <!-- Navbar -->\r\n    <header class=\"relative z-10\">\r\n      <div class=\"mx-auto max-w-7xl px-6 py-5 flex items-center justify-between\">\r\n        <div class=\"flex items-center gap-3\">\r\n          <div class=\"h-10 w-10 rounded-2xl brand-gradient shadow-soft\"></div>\r\n          <span class=\"text-2xl font-bold text-gray-900\">CrimeReport</span>\r\n        </div>\r\n\r\n        <!-- Desktop Nav -->\r\n        <nav class=\"hidden md:flex items-center gap-3\">\r\n          <a href=\"#about\" class=\"nav-link\">About</a>\r\n          <a href=\"#features\" class=\"nav-link\">Features</a>\r\n          <a href=\"#contact\" class=\"nav-link\">Contact</a>\r\n\r\n          <button\r\n            @click=\"openPanic\"\r\n            class=\"ml-4 inline-flex items-center gap-2 px-4 py-2 rounded-xl font-semibold text-white bg-[var(--red)] hover:bg-red-700 focus-ring shadow-soft\"\r\n            aria-label=\"Open panic dialog\"\r\n          >\r\n            <svg class=\"h-4 w-4\" viewBox=\"0 0 24 24\" fill=\"currentColor\" aria-hidden=\"true\"><path d=\"M12 2 1 21h22L12 2zm0 4.9 7.53 13.1H4.47L12 6.9zM11 10h2v5h-2v-5zm0 6h2v2h-2v-2z\"/></svg>\r\n            Panic\r\n          </button>\r\n\r\n          <button @click=\"go('/login')\" class=\"btn-outline\">\r\n            <svg class=\"h-4 w-4 mr-2\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M10 17l5-5-5-5v3H3v4h7v3zm9 4H12v-2h7V5h-7V3h7a2 2 0 012 2v14a2 2 0 01-2 2z\"/></svg>\r\n            Login\r\n          </button>\r\n          <button @click=\"go('/signup')\" class=\"btn-solid\">\r\n            <svg class=\"h-4 w-4 mr-2\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M15 14c2.67 0 8 1.34 8 4v2H7v-2c0-2.66 5.33-4 8-4zM15 12a5 5 0 100-10 5 5 0 000 10z\"/></svg>\r\n            Sign Up\r\n          </button>\r\n        </nav>\r\n\r\n        <!-- Mobile menu button -->\r\n        <button\r\n          @click=\"mobileOpen = !mobileOpen\"\r\n          class=\"md:hidden inline-flex items-center justify-center h-10 w-10 rounded-xl border border-gray-200 hover:bg-gray-50 focus-ring\"\r\n          :aria-expanded=\"mobileOpen ? 'true' : 'false'\"\r\n          aria-label=\"Open navigation menu\"\r\n        >\r\n          <svg v-if=\"!mobileOpen\" class=\"h-5 w-5\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M3 6h18v2H3zM3 11h18v2H3zM3 16h18v2H3z\"/></svg>\r\n          <svg v-else class=\"h-5 w-5\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M18.3 5.71 12 12.01l-6.3-6.3-1.41 1.42 6.3 6.3-6.3 6.3 1.41 1.41 6.3-6.3 6.29 6.3 1.42-1.41-6.3-6.3 6.3-6.3z\"/></svg>\r\n        </button>\r\n      </div>\r\n\r\n      <!-- Mobile Nav -->\r\n      <div v-if=\"mobileOpen\" class=\"md:hidden px-6 pb-4\">\r\n        <div class=\"glass rounded-2xl p-4 shadow-soft\">\r\n          <div class=\"flex flex-col gap-2\">\r\n            <a @click=\"scrollToId('about')\" class=\"nav-link py-2\">About</a>\r\n            <a @click=\"scrollToId('features')\" class=\"nav-link py-2\">Features</a>\r\n            <a @click=\"scrollToId('contact')\" class=\"nav-link py-2\">Contact</a>\r\n          </div>\r\n          <div class=\"mt-3 flex gap-2\">\r\n            <button @click=\"openPanic\" class=\"btn-danger flex-1\">Panic</button>\r\n            <button @click=\"go('/login')\" class=\"btn-outline flex-1\">Login</button>\r\n            <button @click=\"go('/signup')\" class=\"btn-solid flex-1\">Sign Up</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </header>\r\n\r\n    <!-- Hero -->\r\n    <section class=\"relative z-0 mx-auto max-w-7xl px-6 pt-12 pb-10 md:pb-16 flex flex-col md:flex-row items-center gap-10\">\r\n      <div class=\"w-full md:w-1/2\">\r\n        <span class=\"inline-flex items-center px-3 py-1 rounded-full text-sm font-medium text-[var(--blue)] bg-blue-50 border border-blue-100\">Community Safety Platform</span>\r\n        <h1 class=\"mt-4 text-4xl md:text-5xl font-extrabold leading-tight text-gray-900\">\r\n          Report Crimes <span class=\"text-transparent bg-clip-text brand-gradient-text\">Quickly & Safely</span>\r\n        </h1>\r\n        <p class=\"mt-4 text-gray-600 max-w-prose\">\r\n          Empowering neighborhoods with instant reporting of crimes, emergencies, and suspicious activities. Anonymous options, secure data, and real-time alerts.\r\n        </p>\r\n        <div class=\"mt-6 flex flex-wrap gap-3\">\r\n          <button @click=\"openPanic\" class=\"btn-danger\">\r\n            <svg class=\"h-5 w-5 mr-2\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M12 2 1 21h22L12 2zm0 4.9 7.53 13.1H4.47L12 6.9zM11 10h2v5h-2v-5zm0 6h2v2h-2v-2z\"/></svg>\r\n            Panic Button\r\n          </button>\r\n          <button @click=\"go('/signup')\" class=\"btn-solid\">\r\n            Get Started\r\n          </button>\r\n          <button @click=\"go('/login')\" class=\"btn-outline\">\r\n            Login\r\n          </button>\r\n        </div>\r\n\r\n        <!-- Trust badges / quick stats -->\r\n        <div class=\"mt-8 grid grid-cols-2 sm:grid-cols-3 gap-4\">\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-dot bg-[var(--blue)]\"></div>\r\n            <div>\r\n              <p class=\"stat-title\">Anonymous Mode</p>\r\n              <p class=\"stat-value\">Enabled</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"stat-card\">\r\n            <div class=\"stat-dot bg-[var(--green)]\"></div>\r\n            <div>\r\n              <p class=\"stat-title\">24/7 Uptime</p>\r\n              <p class=\"stat-value\">Monitored</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"stat-card col-span-2 sm:col-span-1\">\r\n            <div class=\"stat-dot bg-[var(--red)]\"></div>\r\n            <div>\r\n              <p class=\"stat-title\">Emergency Ready</p>\r\n              <p class=\"stat-value\">1-Tap Panic</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Hero art -->\r\n      <div class=\"w-full md:w-1/2\">\r\n        <div class=\"glass rounded-3xl p-6 md:p-8 shadow-2xl border border-white/50\">\r\n          <div class=\"flex items-center gap-3\">\r\n            <div class=\"h-10 w-10 rounded-xl brand-gradient\"></div>\r\n            <div>\r\n              <p class=\"font-semibold\">Secure Report</p>\r\n              <p class=\"text-sm text-gray-500\">Encrypted & Timestamped</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"mt-6 grid grid-cols-2 gap-3\">\r\n            <div class=\"feature-tile\">\r\n              <svg class=\"h-5 w-5\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M12 1l9 4-9 4-9-4 9-4zm0 7l9 4-9 4-9-4 9-4zm0 7l9 4-9 4-9-4 9-4z\"/></svg>\r\n              Encrypted Storage\r\n            </div>\r\n            <div class=\"feature-tile\">\r\n              <svg class=\"h-5 w-5\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M19 3H5a2 2 0 00-2 2v5a7 7 0 0010.29 6.32l4.6 4.6 1.41-1.41-4.6-4.6A7 7 0 0021 10V5a2 2 0 00-2-2zM5 5h14v5a5 5 0 11-14 0V5z\"/></svg>\r\n              Real-Time Alerts\r\n            </div>\r\n            <div class=\"feature-tile\">\r\n              <svg class=\"h-5 w-5\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M12 12a5 5 0 100-10 5 5 0 000 10zm-7 9a7 7 0 0114 0v1H5v-1z\"/></svg>\r\n              Anonymous Reports\r\n            </div>\r\n            <div class=\"feature-tile\">\r\n              <svg class=\"h-5 w-5\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M2 7h20v2H2V7zm0 4h14v2H2v-2zm0 4h20v2H2v-2z\"/></svg>\r\n              Case Tracking\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Info Section -->\r\n    <section id=\"features\" class=\"mx-auto max-w-7xl px-6 pb-16\">\r\n      <div class=\"grid grid-cols-1 md:grid-cols-3 gap-6\">\r\n        <div class=\"info-card\">\r\n          <h3 class=\"info-title text-[var(--green)]\">Safe</h3>\r\n          <p class=\"info-body\">Your reports are secure and confidential. We never expose personal details without consent.</p>\r\n        </div>\r\n        <div class=\"info-card\">\r\n          <h3 class=\"info-title text-[var(--blue)]\">Reliable</h3>\r\n          <p class=\"info-body\">Built for uptime and speed. Submit reports anytime, from any device.</p>\r\n        </div>\r\n        <div class=\"info-card\">\r\n          <h3 class=\"info-title text-[var(--green)]\">Community</h3>\r\n          <p class=\"info-body\">Work together with local responders and neighbors to keep everyone informed.</p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- About -->\r\n    <section id=\"about\" class=\"mx-auto max-w-5xl px-6 pb-16\">\r\n      <div class=\"glass rounded-3xl p-8 shadow-soft\">\r\n        <h2 class=\"text-2xl font-bold mb-3\">How it works</h2>\r\n        <ol class=\"list-decimal list-inside space-y-2 text-gray-700\">\r\n          <li>Create an account or report anonymously.</li>\r\n          <li>Share what happened, where, and when—attach photos/videos if needed.</li>\r\n          <li>Get a case ID to track updates and responses.</li>\r\n        </ol>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Contact / CTA -->\r\n    <section id=\"contact\" class=\"mx-auto max-w-5xl px-6 pb-16\">\r\n      <div class=\"rounded-3xl p-8 border border-gray-100 bg-gradient-to-r from-blue-50 to-green-50\">\r\n        <h2 class=\"text-2xl font-bold mb-2\">Ready to make your area safer?</h2>\r\n        <p class=\"text-gray-700\">Join the platform and start reporting responsibly.</p>\r\n        <div class=\"mt-4 flex flex-wrap gap-3\">\r\n          <button @click=\"go('/signup')\" class=\"btn-solid\">Create Account</button>\r\n          <button @click=\"go('/login')\" class=\"btn-outline\">I already have an account</button>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Footer -->\r\n    <footer class=\"border-t border-gray-100 text-gray-600 text-center py-6 text-sm\">\r\n      © {{ year }} CrimeReport. All rights reserved.\r\n    </footer>\r\n\r\n    <!-- Panic Modal -->\r\n    <transition name=\"fade\">\r\n      <div\r\n        v-if=\"panicOpen\"\r\n        class=\"fixed inset-0 z-50 flex items-center justify-center p-6\"\r\n        role=\"dialog\"\r\n        aria-modal=\"true\"\r\n        aria-labelledby=\"panic-title\"\r\n        @keydown.esc=\"closePanic\"\r\n      >\r\n        <div class=\"absolute inset-0 bg-black/50\" @click=\"closePanic\"></div>\r\n        <div class=\"relative z-10 w-full max-w-md rounded-2xl bg-white p-6 shadow-2xl\">\r\n          <div class=\"flex items-center gap-3\">\r\n            <div class=\"h-10 w-10 rounded-xl bg-[var(--red)] text-white flex items-center justify-center\">\r\n              <svg class=\"h-6 w-6\" viewBox=\"0 0 24 24\" fill=\"currentColor\"><path d=\"M1 21h22L12 2 1 21zm12-3h-2v-2h2v2zm0-4h-2v-4h2v4z\"/></svg>\r\n            </div>\r\n            <h3 id=\"panic-title\" class=\"text-xl font-bold\">Emergency Panic</h3>\r\n          </div>\r\n          <p class=\"mt-3 text-gray-700\">\r\n            Are you sure you want to send an emergency alert? This will notify responders immediately.\r\n          </p>\r\n          <div class=\"mt-6 flex flex-wrap gap-3\">\r\n            <button @click=\"sendPanic\" class=\"btn-danger flex-1\">Send Alert</button>\r\n            <a :href=\"emergencyTel\" class=\"btn-outline flex-1 text-center\">Call Emergency</a>\r\n            <button @click=\"closePanic\" class=\"btn-ghost flex-1\">Cancel</button>\r\n          </div>\r\n          <p class=\"mt-3 text-xs text-gray-500\">\r\n            Tip: If you're on mobile, your device may vibrate to confirm.\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </transition>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted, onBeforeUnmount, computed } from 'vue'\r\n\r\n// Try to use vue-router if available; fall back to location.href\r\nlet router = null\r\ntry {\r\n  // optional import to avoid hard dependency if not usin
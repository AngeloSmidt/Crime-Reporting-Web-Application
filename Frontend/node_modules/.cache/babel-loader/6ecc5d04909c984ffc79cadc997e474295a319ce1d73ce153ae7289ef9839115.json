{"ast":null,"code":"import { useRoute } from 'vue-router';\nimport { computed } from 'vue';\n\n// Component imports\nimport Navbar from './components/Navbar.vue';\nimport Footer from './components/Footer.vue';\nimport AdminNavbar from './components/AdminNavbar.vue';\nimport AdminFooter from './components/AdminFooter.vue';\nexport default {\n  name: 'App',\n  components: {\n    Navbar,\n    Footer,\n    AdminNavbar,\n    AdminFooter\n  },\n  setup() {\n    const route = useRoute();\n\n    // Routes that should have NO layout at all\n    const noLayoutRoutes = ['/login', '/register', '/admin-login'];\n\n    // Routes that belong to the admin layout\n    const adminRoutes = ['/admin-dashboard', '/admin-contacts', '/admin-settings', '/admin-users'\n    // Add any other admin paths here\n    ];\n\n    // Show user layout if route is NOT in noLayoutRoutes or adminRoutes\n    const showUserLayout = computed(() => !noLayoutRoutes.includes(route.path) && !adminRoutes.includes(route.path));\n\n    // Show admin layout if route is in the adminRoutes list\n    const showAdminLayout = computed(() => adminRoutes.includes(route.path));\n    return {\n      showUserLayout,\n      showAdminLayout\n    };\n  }\n};","map":{"version":3,"names":["useRoute","computed","Navbar","Footer","AdminNavbar","AdminFooter","name","components","setup","route","noLayoutRoutes","adminRoutes","showUserLayout","includes","path","showAdminLayout"],"sources":["C:\\Users\\sifis\\OneDrive\\Documents\\Crime-Reporting-Web-Application\\Frontend\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <!-- User Layout -->\r\n    <Navbar v-if=\"showUserLayout\" />\r\n\r\n    <!-- Admin Layout -->\r\n    <AdminNavbar v-else-if=\"showAdminLayout\" />\r\n\r\n    <main>\r\n      <router-view />\r\n    </main>\r\n\r\n    <!-- User Footer -->\r\n    <Footer v-if=\"showUserLayout\" />\r\n\r\n    <!-- Admin Footer -->\r\n    <AdminFooter v-else-if=\"showAdminLayout\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRoute } from 'vue-router'\r\nimport { computed } from 'vue'\r\n\r\n// Component imports\r\nimport Navbar from './components/Navbar.vue'\r\nimport Footer from './components/Footer.vue'\r\nimport AdminNavbar from './components/AdminNavbar.vue'\r\nimport AdminFooter from './components/AdminFooter.vue'\r\n\r\nexport default {\r\n  name: 'App',\r\n  components: {\r\n    Navbar,\r\n    Footer,\r\n    AdminNavbar,\r\n    AdminFooter\r\n  },\r\n  setup() {\r\n    const route = useRoute()\r\n\r\n    // Routes that should have NO layout at all\r\n    const noLayoutRoutes = ['/login', '/register', '/admin-login']\r\n\r\n    // Routes that belong to the admin layout\r\n    const adminRoutes = [\r\n      '/admin-dashboard',\r\n      '/admin-contacts',\r\n      '/admin-settings',\r\n      '/admin-users'\r\n      // Add any other admin paths here\r\n    ]\r\n\r\n    // Show user layout if route is NOT in noLayoutRoutes or adminRoutes\r\n    const showUserLayout = computed(() =>\r\n      !noLayoutRoutes.includes(route.path) && !adminRoutes.includes(route.path)\r\n    )\r\n\r\n    // Show admin layout if route is in the adminRoutes list\r\n    const showAdminLayout = computed(() =>\r\n      adminRoutes.includes(route.path)\r\n    )\r\n\r\n    return {\r\n      showUserLayout,\r\n      showAdminLayout\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n/* Optional global styles */\r\nbody {\r\n  margin: 0;\r\n  font-family: 'Inter', sans-serif;\r\n}\r\n</style>\r\n"],"mappings":"AAqBA,SAASA,QAAO,QAAS,YAAW;AACpC,SAASC,QAAO,QAAS,KAAI;;AAE7B;AACA,OAAOC,MAAK,MAAO,yBAAwB;AAC3C,OAAOC,MAAK,MAAO,yBAAwB;AAC3C,OAAOC,WAAU,MAAO,8BAA6B;AACrD,OAAOC,WAAU,MAAO,8BAA6B;AAErD,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVL,MAAM;IACNC,MAAM;IACNC,WAAW;IACXC;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIT,QAAQ,CAAC;;IAEvB;IACA,MAAMU,cAAa,GAAI,CAAC,QAAQ,EAAE,WAAW,EAAE,cAAc;;IAE7D;IACA,MAAMC,WAAU,GAAI,CAClB,kBAAkB,EAClB,iBAAiB,EACjB,iBAAiB,EACjB;IACA;IAAA,CACF;;IAEA;IACA,MAA